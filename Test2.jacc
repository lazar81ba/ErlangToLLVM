    private static void yyerror(int line, String message)  { //Nie wiem czy zadziala
        report(line, "", message);
    }

    private static void report(int line, String where, String message) {
        System.err.println(
                "[line " + line + "] Error" + where + ": " + message);
        hadError = true;
